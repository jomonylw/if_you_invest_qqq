{
  "crons": [
    {
      "schedule": "0 9 * * *",
      "path": "/api/update"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "installCommand": "pnpm install",
        "buildCommand": "npx prisma generate && pnpm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app/page.tsx"
    }
  ]
}